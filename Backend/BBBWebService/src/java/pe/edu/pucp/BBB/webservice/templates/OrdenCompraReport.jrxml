	<parameter name="ID_comprobante" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
  o.id_orden, 
  o.estado, 
  o.fecha_creacion, 
  o.total,
  oc.id_orden_compra, 
  oc.fecha_recepcion,
  lo.id_producto, 
  p.nombre AS nombre_producto, 
  p.precio_unitario, 
  p.stock, 
  p.capacidad, 
  p.unidad_medida, 
  p.tipo, lo.cantidad, 
  lo.subtotal
FROM Orden_Compra oc
LEFT JOIN Orden o ON o.id_orden = oc.id_orden
LEFT JOIN LineaOrden lo ON o.id_orden = lo.id_orden
LEFT JOIN Producto p ON lo.id_producto = p.id_producto
WHERE o.id_orden IS NOT NULL
  AND oc.id_orden_compra = $P{ID_comprobante}]]>
	</queryString>
